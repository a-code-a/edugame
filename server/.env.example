# Backend Environment Variables
# Copy this file to .env and fill in your values

# MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/

# Frontend URL for CORS (leave empty for local development)
# In production on Render, this will be set automatically
FRONTEND_URL=

# Server port (Render will set this automatically in production)
PORT=5000

# Node environment
NODE_ENV=development
